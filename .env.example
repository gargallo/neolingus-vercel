# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Update.dev Configuration  
NEXT_PUBLIC_UPDATE_PUBLISHABLE_KEY=your_update_publishable_key

# AI Provider API Keys
# OpenAI (Required for GPT models)
OPENAI_API_KEY=your_openai_api_key

# Anthropic (Optional - for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Google AI (Optional - for Gemini models)
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key

# Agent Configuration (Optional)
# Default model provider if not specified in agent config
DEFAULT_AI_PROVIDER=openai
# Default model name if not specified
DEFAULT_AI_MODEL=gpt-4-turbo

# Rate Limiting (Optional)
# Max requests per minute per agent
AGENT_RATE_LIMIT=60
# Max tokens per minute per agent
AGENT_TOKEN_LIMIT=100000

# Monitoring (Optional)
# Enable detailed logging for agent interactions
AGENT_DEBUG_MODE=false
# Store all agent interactions in database
AGENT_STORE_INTERACTIONS=true

# Demo Mode (Optional)
# Enable demo mode to bypass authentication for testing purposes
# WARNING: Only use in development environments
DEMO_MODE=false